{
  "manifestVersion": 1,
  "id": "Appknox",
  "name": "Appknox",
  "description": "Appknox Automated Security Scanning",
  "version": "2.0.14",
  "publisher": "Appknox",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "mobile",
    "security",
    "sast",
    "dast",
    "plugin",
    "appknox",
    "vulnerability",
    "scan",
    "mobile security",
    "automated scanner",
    "Azure Pipelines",
    "Azure DevOps Extensions",
    "Marketplace"
  ],
  "public": true,
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "files": [
    {
      "path": "buildAndReleaseTask"
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "overview.md",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "appknox",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "buildAndReleaseTask"
      }
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE.md"
    }
  },
  "links": {
    "home": {
      "uri": "https://www.appknox.com/"
    },
    "repository": {
      "uri": "https://github.com/appknox/appknox-azure-extension"
    },
    "issues": {
      "uri": "https://github.com/appknox/appknox-azure-extension/issues"
    },
    "support": {
      "uri": "mailto:support@appknox.com"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/appknox/appknox-azure-extension"
  },
  "branding": {
    "color": "rgb(00, 153, 255)",
    "theme": "dark"
  },
  "screenshots": [
    {
      "path": "images/basic-config.png"
    },
    {
      "path": "images/marketplace.png"
    },
    {
      "path": "images/install.png"
    },
    {
      "path": "images/variable.png"
    },
    {
      "path": "images/logs.png"
    }
  ]
}
